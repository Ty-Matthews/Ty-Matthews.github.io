{
	"$schema": "https://json-schema.org/draft/2020-12/schema", 
	"$id": "https://ty-matthews.github.io/Wally/json/schema/texture_properties.wal.json",
	"description": "A schema for describing bitmask flags for the q2_miptex struct variable.",
{
  "type": "object",
  "required": ["entries"],
  "properties": {
	  "entries": {
		  "type": "array",
		  "items": {
			  "type": "object",
			  "properties": {
				"name": { "type": "string" },
				"bitmask": { "type": "number" }
			  },
			  "required": ["name", "bitmask"]
		  }
	  }
  }
}